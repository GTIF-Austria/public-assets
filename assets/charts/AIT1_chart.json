{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Vertical deflection over time",
    "data": {
      "name": "timeseries"
    },
    "mark": {
      "type": "line",
      "point": {
        "filled": true,
        "fill": "blue"
      },
      "tooltip": true
    },
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "title": "Date"
      },
      "y": {
        "field": "deflection",
        "type": "quantitative",
        "title": "Vertical Deflection (mm)"
      },
      "tooltip": [
        {"field": "date", "type": "temporal", "title": "Date"},
        {"field": "deflection", "type": "quantitative", "title": "Deflection (mm)"}
      ]
    }
  }
  